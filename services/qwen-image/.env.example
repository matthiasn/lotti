# Qwen Image Service Configuration

# Server settings
HOST=127.0.0.1
PORT=11345
LOG_LEVEL=INFO

# Model settings
QWEN_IMAGE_MODEL_ID=Qwen/Qwen-Image

# Device selection (auto, cuda, mps, cpu)
# auto = automatically select best available device
QWEN_IMAGE_DEVICE=auto

# Optional: HuggingFace token for faster downloads
# Not required for Qwen-Image (Apache 2.0 license, not gated)
# HF_TOKEN=your_token_here

# Image generation defaults
QWEN_IMAGE_WIDTH=1664
QWEN_IMAGE_HEIGHT=928
QWEN_IMAGE_STEPS=50
QWEN_IMAGE_CFG_SCALE=4.0
QWEN_IMAGE_TIMEOUT=300

# Performance tuning
MAX_CONCURRENT_REQUESTS=1
LOG_TO_STDOUT=false
